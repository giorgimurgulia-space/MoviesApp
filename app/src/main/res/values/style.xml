<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="normalMontserratMedium">
        <item name="android:textSize">@dimen/dimen_14_sp</item>
        <item name="fontFamily">@font/montserrat_medium</item>
        <item name="lineHeight">@dimen/dimen_18_sp</item>
    </style>

    <style name="mediumMontserratMedium">
        <item name="android:textSize">@dimen/dimen_16_sp</item>
    </style>

    <style name="smallMontserratMedium">
        <item name="android:textSize">@dimen/dimen_12_sp</item>
    </style>

    <style name="CustomChipStyle" parent="Widget.MaterialComponents.Chip.Choice">
        <item name="chipBackgroundColor">@color/chip_background_color_selector</item>
        <item name="chipStrokeColor">@color/chip_stroke_color_selector</item>
        <item name="android:textColor">@color/chip_text_color_selector</item>
        <item name="chipStrokeWidth">@dimen/dimen_1_dp</item>
    </style>

</resources>